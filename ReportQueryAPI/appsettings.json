{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1AndHttp2"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server={SQL_SERVER};Database={SQL_DATABASE};User ID={SQL_USER};Password={SQL_PASSWORD};TrustServerCertificate=True"
  }
}